    "@types/jsonwebtoken": "^9.0.10",
    "@vercel/node": "^5.6.3",
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^7.1.0",
    "rxjs": "^7.8.1",
    "tailwindcss": "latest",
    "zone.js": "^0.16.0"
